<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta http-equiv="x-ua-compatible" content="ie=edge">
    <meta name="viewport"
          content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <!-- Use the title from a page's frontmatter if it has one -->
    <title><%= current_page.data.title || "IORGU SPRL" %></title>
    <%= stylesheet_link_tag "site" %>
      <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">

    <%= javascript_include_tag '//cdnjs.cloudflare.com/ajax/libs/jquery/3.2.1/jquery.min.js',"//cdnjs.cloudflare.com/ajax/libs/materialize/0.98.2/js/materialize.min.js","all" %>
    <meta name="description" content="Iorgu sprl, habillage fenÃªtre et nettoyage" />

  </head>
  <body class="grey lighten-4">
    <nav class="white">
    <div class="nav-wrapper">
      <a href="/" class="brand-logo center"><%= image_tag "logov2.png",height: 61 %></a>
      <ul id="nav-mobile" class="left hide-on-med-and-down">
      <li class="black-text"><%= link_to "A-propos de nous","about",class: "black-text" %></li>

      <% if !current_page.data.navbar.nil? %>
        <% current_page.data.navbar.each do |link| %>
          <li><%= link_to link,link %></li>
        <% end %>
        <% end %>
      </ul>
    </div>
  </nav>
    <%= yield %>
  </body>
</html>
